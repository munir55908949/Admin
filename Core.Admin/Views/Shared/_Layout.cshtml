    <script>
        InializeDateH();

        function InializeDateH() {
            //var calendar = $.calendars.instance('ummalqura', 'ar');
            $('.datepickerPopup').calendarsPicker({ todayText: "اليوم", prevText: "السابق >>", nextText: "<< التالي", closeText: "اغلاق", clearText: "مسح", dateFormat: "yyyy/mm/dd" });
            $('.datepickerPopup').attr("readonly", "readonly");
        }

        var RowNumber = 0;
        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;

            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }
        function isDecimalNumber(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode

            if (charCode != 45 && (charCode != 46 ||
                    $(this).val().indexOf('.') != -1) && (charCode < 48 || charCode > 57))
                return false;

            return true;
        }
        function CheckArabicEngCharactersOnly(e) {
            var unicode = e.charCode ? e.charCode : e.keyCode
            if (unicode != 8) { //if the key isn't the backspace key (which we should allow)
                if (unicode == 32) {
                    return true;
                }
                else {
                    if ((unicode < 48 || unicode < 57)
                        && (unicode < 0x0600 || unicode > 0x06FF)) //if not a number or arabic
                    {
                        return false;
                    }
                    //disable key press
                }
            }
        }
        function onSelect(e) {
            $.each(e.files, function (index, value) {
                if (value.extension.toLowerCase() != ".jpg"
                   && value.extension.toLowerCase() != ".jpeg"
               && value.extension.toLowerCase() != ".png") {
                    swal("", "امتداد الملف فقط يكون jpg,png,jpeg", "warning");
                    e.preventDefault();
                }
            });
        };
        jQuery(document).ready(function () {
            Main.init();
            UIModals.init();
            //-----------------------------//

            $(window).keydown(function (event) {
                if (event.keyCode == 13) {
                    $('#btnSearch').click();
                    return false;
                }
            });

        });
        function ShowLoading() {
            var validator = $("#frmAddEdit").kendoValidator().data('kendoValidator');
            if (validator.validate()) {
                var $form1 = $('#frmAddEdit');
                if ($form1.valid()) {
                    $("#loadingPanel").show();
                    return true;
                }
            }
        }
        function IsEnglishOnly(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            var englishAlphabetAndWhiteSpace = /[A-Za-z]/g;

            var key = String.fromCharCode(evt.which);
            //(charCode != 46 ||
            //        $(this).val().indexOf('.') != -1) &&
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                if (englishAlphabetAndWhiteSpace.test(key)) {

                    return true;
                }
                return false;
            }
            return true;
        }
        $(function () {
            if (localStorage.getItem("navigationSmall")) {
                $("body").addClass("navigation-small");
            }
        });
        $(".navigation-toggler").click(function () {
            if (localStorage.getItem("navigationSmall")) {
                localStorage.removeItem("navigationSmall", "navigation-small")
            }
            else
                localStorage.setItem("navigationSmall", "navigation-small")

        });
    </script>
