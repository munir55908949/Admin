                                                                <div class="col-md-12">
                                                                    <div class="col-md-2">
                                                                        <div class="form-group">
                                                                            <label class="control-label">
                                                                                ملاحظـــــات
                                                                            </label>
                                                                            @Html.TextBoxFor(m => Model.NoteWarranty, new { @class = "form-control", @placeholder = "البحــث بالملاحظــات" })
                                                                        </div>
